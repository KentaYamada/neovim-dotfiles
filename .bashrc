# env

# command alias
# plain commands
alias ..='cd ../'
alias cp='cp -i'
alias sl='ln -s'
alias usl='unlink'
alias ls='ls -al'


# git commands
alias ga='git add'
alias gaa='git add .'
alias gb='git branch'
alias gc='git checkout'
alias gcb='git checkout -b'
alias gl='git log'
alias gp='git push'
alias gpu='git push --set-upstream'
alias gs='git status'

# python command
alias py='python3 -B'
