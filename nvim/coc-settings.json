{
    "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "python", "go"],
    "languageserver": {
        "golang": {
            "command": "gopls",
            "filetypes": ["go"]
        }
    },
    "eslint.autoFixOnSave": true,
    "eslint.probe": [
        "javascript",
        "typescript"
    ],
    "tsserver.enable": true,
    "python.linting.flake8Enabled": true,
    "python.formatting.provider": "black",
    "clangd.arguments": ["-W"]
}
